!! URxvt
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
URxvt.scrollBar:		false
URxvt.secondaryScroll:  	true
URxvt*mouseWheelScrollPage:     false
URxvt.loginShell:		true
URxvt.saveLines:  		5000
URxvt.cursorBlink:  		true
URxvt.urgentOnBell:  		false
URxvt.visualBell:  		false
URxvt.letterSpace:  		0
URxvt.internalBorder:  		1
URxvt.pointerBlank:  		true
URxvt.hold:  			false
URxvt.buffered:  		true
URxvt.depth:			32
URxvt*background:       	black
URxvt*foreground:       	grey

! Copy-paste keybdings
URxvt.iso14755: 		false
URxvt.iso14755_52: 		false
URxvt.keysym.Shift-Control-V:   eval:paste_clipboard
URxvt.keysym.Shift-Control-C:   eval:selection_to_clipboard

URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B

!! Fonts
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
Xft.dpi: 	96
Xft.antialias: 	true
Xft.hinting: 	true
Xft.rgba: 	rgb
Xft.autohint: 	false
Xft.hintstyle: 	hintslight
Xft.lcdfilter:	lcddefault
URxvt.font:	9x15,xft:TerminessTTFNerdFontMono
!URxvt.font    : xft:monospace:size=9:antialias=true
!URxvt.font    : xft:Fira\ Mono:size=9:antialias=true
!URxvt.font    : xft:Source Code Pro:size=9:antialias=true

